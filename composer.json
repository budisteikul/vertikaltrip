{
    "name": "budisteikul/vertikaltrip",
    "description": "Vertikal Trip SDK",
    "authors": [
        {
            "name": "Budi",
            "email": "aku@budi.my.id"
        }
    ],
    "require": {
        "ext-gd": "*",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "barryvdh/laravel-dompdf": "^3.0",
        "google/cloud-tasks": "^1.14",
        "html2text/html2text": "^4.3",
        "intervention/image": "^3.8",
        "predis/predis": "^2.2",
        "propaganistas/laravel-phone": "^5.3",
        "spatie/laravel-google-cloud-storage": "^2.3",
        "yajra/laravel-datatables-oracle": "^11.1",
        "stripe/stripe-php": "^15.8",
        "symfony/http-client": "^7.1",
        "symfony/mailgun-mailer": "^7.1"
    },
    "autoload": {
        "psr-0": {
            "SimpleSoftwareIO\\QrCode\\": "src/Libs/",
            "BaconQrCode": "src/Libs/"
        },
        "psr-4": {
            "budisteikul\\vertikaltrip\\": "src/",
            "PayPalCheckoutSdk\\": "src/Libs/PayPalCheckoutSdk/",
            "PayPalHttp\\": "src/Libs/PayPalHttp/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "budisteikul\\vertikaltrip\\VertikaltripServiceProvider",
                "SimpleSoftwareIO\\QrCode\\QrCodeServiceProvider",
                "Barryvdh\\DomPDF\\ServiceProvider"
            ],
            "aliases": {
                "QrCode": "SimpleSoftwareIO\\QrCode\\Facades\\QrCode",
                "PDF": "Barryvdh\\DomPDF\\Facade\\Pdf"
            }
        }
    }
}